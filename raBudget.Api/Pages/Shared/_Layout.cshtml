@using Microsoft.AspNetCore.Hosting
@using Microsoft.AspNetCore.Mvc.ViewEngines
@inject IWebHostEnvironment Environment
@inject ICompositeViewEngine Engine
@using raBudget.Api.Infrastructure
<!DOCTYPE html>
<html class="h-100">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - raBudget</title>

    
    <link href="~/dist/vendor.bootstrap-material-design.e51fd2ef4a5db20a6480.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500" rel="stylesheet">
    
    <link href="~/dist/main.d75a303aea00d73df80a.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500" rel="stylesheet">
    
    <script src="https://code.iconify.design/1/1.0.7/iconify.min.js"></script>

</head>
<body class="h-100 mdc-typography mdc-theme--background">
    <div class="container h-100">
        <main role="main" class="pb-3 h-100">
            @RenderBody()
        </main>
    </div>
    
    
    <script src="~/dist/vendor.material.36ab9bc28797354368da.js"></script>
    
    <script src="~/dist/vendor.tslib.36ab9bc28797354368da.js"></script>
    
    <script src="~/dist/main.36ab9bc28797354368da.js"></script>
    
    @RenderSection("_script", required: false)
    @RenderSection("Scripts", required: false)
    @Html.RenderScripts()

</body>
</html>
