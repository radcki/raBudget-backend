{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "removeComments": true,
    "allowJs": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [  ],
    "paths": {
      "@/*": [ "src/*" ]
    },
    "lib": [ "es2019", "es2016", "es5", "dom", "dom.iterable", "scripthost" ]
  },
  "include": [ "**/*.ts", "**/*.tsx" ],
  "exclude": [ "node_modules" ]
}